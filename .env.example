# Database connection string used by dbmate. Adjust to your local environment.
DATABASE_URL=postgres://postgres:postgres@localhost:5432/keepon_solito_dev?sslmode=disable

# Workflow worlds
# Leave WORKFLOW_TARGET_WORLD unset to let Workflow auto-select:
# - Local World for local dev
# - Vercel World on Vercel
# WORKFLOW_TARGET_WORLD=
# WORKFLOW_POSTGRES_URL=postgres://postgres:postgres@localhost:5432/keepon_solito_dev?sslmode=disable
WORKFLOW_OUTBOX_AFTER_ENABLED=true
WORKFLOW_OUTBOX_AFTER_LIMIT=5
WORKFLOW_OUTBOX_DISPATCHER_ENABLED=true
WORKFLOW_OUTBOX_DISPATCH_CONCURRENCY=4
WORKFLOW_OUTBOX_CLAIM_BATCH_SIZE=20
WORKFLOW_OUTBOX_DISPATCH_SECRET=
CRON_SECRET=

# Optional: additional .env values go here.
NEXT_PUBLIC_ORPC_BASE_URL=http://localhost:3001
EXPO_PUBLIC_ORPC_BASE_URL=http://localhost:3001

# Public base URL for webhook callbacks
BASE_URL=http://localhost:3001
BOOKING_SHORT_URL=

# App Store
APP_STORE_SHARED_SECRET=

# Mandrill
MANDRILL_API_KEY=

# Mailchimp
MAILCHIMP_API_KEY=
MAILCHIMP_AUDIENCE_ID=
MAILCHIMP_SERVER_PREFIX=us3

# Twilio
TWILIO_ACCOUNT_SID=
TWILIO_AUTH_TOKEN=
TWILIO_MESSAGING_SERVICE_SID=

# Apple Push Notifications
APNS_KEY=
APNS_KEY_ID=
APNS_TEAM_ID=
APNS_PRODUCTION=true
IOS_BUNDLE_ID=
