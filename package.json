{
  "name": "keepon-solito",
  "private": true,
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "devDependencies": {
    "@eslint/js": "^9.39.1",
    "@react-native/eslint-plugin": "^0.82.1",
    "@types/react": "19.2.7",
    "dbmate": "2.28.0",
    "eslint": "9.39.1",
    "eslint-config-expo": "^10.0.0",
    "eslint-config-next": "16.0.5",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-react-compiler": "19.1.0-rc.2",
    "kysely-codegen": "0.19.0",
    "patch-package": "^8.0.1",
    "pg": "^8.16.3",
    "turbo": "^2.6.1",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.48.0"
  },
  "scripts": {
    "native": "yarn workspace expo-app start",
    "web": "yarn workspace next-app dev",
    "postinstall": "patch-package",
    "dbmate": "dbmate",
    "dbmate:new": "dbmate new",
    "dbmate:up": "dbmate up",
    "dbmate:down": "dbmate rollback",
    "dbmate:status": "dbmate status",
    "dbmate:reset": "dbmate drop && dbmate up",
    "db:types": "kysely-codegen",
    "lint": "eslint . --max-warnings=0"
  },
  "packageManager": "yarn@1.22.22",
  "prettier": {
    "semi": false,
    "useTabs": false,
    "tabWidth": 2,
    "singleQuote": true
  },
  "dependencies": {
    "@legendapp/list": "^3.0.0-beta.2"
  }
}
