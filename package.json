{
  "name": "next-app",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "build": "next build",
    "ci:deploy": "bash scripts/ci/deploy.sh",
    "check:workflow-parity": "tsx scripts/check-workflow-parity.ts",
    "codegen": "kysely-codegen",
    "db:migrate": "dbmate --migrations-dir db/migrations up",
    "db:migrate:prod": "dbmate --no-dump-schema --migrations-dir db/migrations -e DATABASE_URL_PROD up",
    "dev": "next dev --turbo -p 3001",
    "format": "oxfmt --write .",
    "format:check": "oxfmt --check .",
    "lint": "oxlint --type-aware --type-check .",
    "start": "next start -p 3001",
    "sync:reference-data": "tsx scripts/sync-reference-data.ts"
  },
  "dependencies": {
    "@base-ui/react": "1.1.0",
    "@google-cloud/storage": "^7.18.0",
    "@mailchimp/mailchimp_marketing": "^3.0.80",
    "@orpc/client": "1.13.4",
    "@orpc/server": "1.13.4",
    "@orpc/tanstack-query": "1.13.4",
    "@parse/node-apn": "^7.1.0",
    "@stripe/react-stripe-js": "^5.6.0",
    "@stripe/stripe-js": "^8.7.0",
    "@workflow/world-postgres": "4.1.0-beta.30",
    "bignumber.js": "^9.3.1",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "date-fns": "^4.1.0",
    "file-type": "^21.3.0",
    "kysely": "0.28.11",
    "libphonenumber-js": "^1.12.36",
    "lucide-react": "^0.563.0",
    "next": "16.1.6",
    "pg": "^8.18.0",
    "postgres-interval": "^4.0.2",
    "react": "19.2.4",
    "react-dom": "19.2.4",
    "sharp": "^0.34.5",
    "stripe": "20.3.0",
    "tailwind-merge": "^3.4.0",
    "uuid": "^13.0.0",
    "workflow": "4.1.0-beta.52",
    "zod": "4.3.6"
  },
  "devDependencies": {
    "@babel/core": "^7.29.0",
    "@tailwindcss/postcss": "^4.1.18",
    "@types/node": "25.2.0",
    "@types/pg": "^8.16.0",
    "@types/react": "^19.2.11",
    "@typescript/native-preview": "7.0.0-dev.20260204.1",
